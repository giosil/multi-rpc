  "servers": [
    {
      "url": "http://localhost:8080/wdemo-be/rpc"
    }
  ],
  "paths": {
    "/CONTACTS.list": {
      "get": {
        "summary": "list contacts",
        "description": "list contacts",
        "operationId": "CONTACTS.list",
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Contact"
                  }
                }
              }
            }
          },
          "500": {
            "description": "Internal Error"
          }
        }
      }
    },
    "/CONTACTS.find": {
      "get": {
        "summary": "find contacts",
        "description": "find contacts",
        "operationId": "CONTACTS.find",
        "parameters": [
          {
            "name": "name",
            "in": "query",
            "description": "Contact name that need to be considered for filter",
            "required": false,
            "explode": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "email",
            "in": "query",
            "description": "Contact email that need to be considered for filter",
            "required": false,
            "explode": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Contact"
                  }
                }
              }
            }
          },
          "500": {
            "description": "Internal Error"
          }
        }
      }
    },
    "/CONTACTS.clear": {
      "get": {
        "summary": "clear contacts",
        "description": "clear contacts",
        "operationId": "CONTACTS.clear",
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "boolean"
                }
              }
            }
          },
          "500": {
            "description": "Internal Error"
          }
        }
      }
    },
    "/CONTACTS.init": {
      "get": {
        "summary": "init application",
        "description": "init application",
        "operationId": "CONTACTS.init",
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "integer",
                  "format": "int32"
                }
              }
            }
          },
          "500": {
            "description": "Internal Error"
          }
        }
      }
    },
    "/CONTACTS.add": {
      "post": {
        "summary": "add a contact",
        "description": "add a contact",
        "operationId": "CONTACTS.add",
        "requestBody": {
          "description": "Contact to add",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Contact"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "boolean"
                }
              }
            }
          },
          "500": {
            "description": "Internal Error"
          }
        }
      }
    },
    "/CONTACTS.remove": {
      "delete": {
        "summary": "remove a contact",
        "description": "remove a contact",
        "operationId": "CONTACTS.remove",
        "parameters": [
          {
            "name": "name",
            "in": "query",
            "description": "Contact name that need to be considered for filter",
            "required": false,
            "explode": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "email",
            "in": "query",
            "description": "Contact email that need to be considered for filter",
            "required": false,
            "explode": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "integer",
                  "format": "int32"
                }
              }
            }
          },
          "500": {
            "description": "Internal Error"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Contact": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "Contact name"
          },
          "email": {
            "type": "string",
            "description": "Contact email"
          }
        }
      }
    }
  }
}